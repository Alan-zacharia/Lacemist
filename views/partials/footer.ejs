<script>
    async function addtoCart(userId, productId) {
        try {

            const response = await fetch('/addToCart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({
                    productId,
                    userId,
                }),
            });
            if (response.ok) {
                Swal.fire({
                    title: 'success',
                    icon: 'success',
                    text: 'Added to cart successfull'
                })
            } else {
                Swal.fire({
                    title: 'Fail',
                    icon: 'Fail',
                    text: 'Added to cart failed please login now'
                })
            }
        } catch (error) {
            console.error('An error occurred:', error);
            alert('An error occurred while adding the product to the cart.');
        }
    }

    async function addroWhislist(userId, productId) {
        try {
            const response = await fetch('/whislist', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    userId,
                    productId
                }),
            })
            if (response.ok) {
                Swal.fire({
                    title: 'success',
                    icon: 'success',
                    text: 'Added to whislist successfull'
                })
            } else {
                Swal.fire({
                    title: 'Fail',
                    icon: 'Fail',
                    text: 'Added to whislist failed please login now'
                })
            }
        } catch (error) {
            
            console.log(error.message);
           
        }
    }
</script>
